{
	"name": "e-commerce",
	"private": true,
	"version": "1.0.0",
	"description": "A simple e-commerce website to learn how to use react's hook context",
	"type": "module",
	"scripts": {
		"start": "concurrently --names \"SERVER,VITE\" --prefix-colors \"green,cyan\" --prefix name \"npm run db\" \"npm run dev\"",
		"db": "json-server --watch db.json --port 3001",
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"dependencies": {
		"json-server": "^1.0.0-alpha.23",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.22.3"
	},
	"devDependencies": {
		"@types/react": "^18.2.66",
		"@types/react-dom": "^18.2.22",
		"@vitejs/plugin-react": "^4.2.1",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"vite": "^5.2.0"
	}
}
